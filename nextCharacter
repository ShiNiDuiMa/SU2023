import java.util.Scanner;

public class Input {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		char a = (char)(sc.next().charAt(0) + 1);
		// char c = (char) (a + 1);
		System.out.println(a);
		// char c = sc.nextLine().charAt(0);
		// c = (char)((int)c + 1);
	}
}
